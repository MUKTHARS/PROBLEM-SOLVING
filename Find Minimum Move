 int findminmove(int a[],int n)
    {
        int ecount =0 , ocount =0;
        for(int i=0;i<n;i++)
        {
            if(a[i]%2==0)
            {
                ++ecount;
            }
        }
          for(int i =0;i<ecount;i++)
    {
        if(a[i]%2 ==1)
        {
        ++ocount;
        }
    }
    return ecount;
    }
  
